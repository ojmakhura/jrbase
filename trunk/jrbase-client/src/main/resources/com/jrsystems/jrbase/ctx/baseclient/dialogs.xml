<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	
	<bean id="individualDialog" class="com.jrsystems.jrbase.individual.IndividualDialog">
		<property name="itemForm" ref="individualForm"/>
	</bean>
	
	<bean id="userDialog" class="com.jrsystems.jrbase.user.UserDialog">
		<property name="itemForm" ref="userForm"/>
	</bean>
	
	<bean id="counterDialog" class="com.jrsystems.jrbase.counter.CounterDialog">
		<property name="itemForm" ref="counterForm"/>
	</bean>
	
	<bean id="clearanceLevelDialog" class="com.jrsystems.jrbase.clearancelevel.ClearanceLevelDialog">
		<property name="itemForm" ref="clearanceLevelForm"/>
	</bean>
	
	<bean id="roleDialog" class="com.jrsystems.jrbase.role.RoleDialog">
		<property name="itemForm" ref="roleForm"/>
	</bean>
	
	<bean id="locationDialog" class="com.jrsystems.jrbase.location.LocationDialog">
		<property name="itemForm" ref="locationForm"/>
	</bean>
	
	<bean id="locationTypeDialog" class="com.jrsystems.jrbase.location.LocationTypeDialog">
		<property name="itemForm" ref="locationTypeForm"/>
	</bean>
	
	<bean id="organisationDialog" class="com.jrsystems.jrbase.organisation.OrganisationDialog">
		<property name="itemForm" ref="organisationForm"/>
	</bean>
	
	<bean id="organisationTypeDialog" class="com.jrsystems.jrbase.organisation.OrganisationTypeDialog">
		<property name="itemForm" ref="organisationTypeForm"/>
	</bean>
</beans>